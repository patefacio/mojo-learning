   {
     "name": "Mojo Learning Environment",
     "build": {
       "dockerfile": "Dockerfile"
     },
     "features": {
       "ghcr.io/devcontainers/features/python:1": {
         "version": "3.11"
       }
     },
     "postCreateCommand": "pip install jupyter notebook",
     "forwardPorts": [8888],
     "customizations": {
       "vscode": {
         "extensions": [
           "ms-python.python",
           "ms-toolsai.jupyter"
         ]
       }
     }
   }
   
